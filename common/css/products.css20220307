#products.sub article#products-link { margin:0 auto 60px; width:100%; padding-top:0;}
.products-link { border-top:2px solid #D9E8F4; margin:60px 0 0 0; padding:60px 0 0 0;  width:100%;}
.products-link h2 { font-size:1.125rem; line-height:100%; font-weight:bold; margin:0 0 30px 0;}
.products-link ul { width:1100px; margin:0 auto; padding-bottom:40px; justify-content:flex-start; /*justify-content:space-between;*/ /*webkit-align-content:flex-start; align-content:flex-start;*/}
.products-link ul li { width:20%; box-sizing:border-box; padding-right:20px; margin-bottom:30px;}
.products-link ul li a { text-decoration:none;}
.products-link ul li a img { width:auto; height:90px; float:left; margin:0 10px 0 0;}
.products-link ul li a h3 { font-size:0.813rem; line-height:120%; text-align:left; padding:20px 0 0 0; text-align:left;}
.products-link ul li a:hover { text-decoration:none;}
.products-link ul li:nth-last-child(-n+5) { margin-bottom:0;}
@media screen and (max-width:900px){
	.products-link { margin:40px 0 0 0; padding:40px 0 30px 0;}
	.products-link ul { width:90%;}
	.products-link ul li { width:25%; box-sizing:border-box; padding:0 0 30px; margin:0;}
	.products-link ul li a img { width:auto; height:90px; float:none; margin:0 auto 0px;}
	.products-link ul li:nth-last-child(-n+5) { margin-bottom:30px;}
	.products-link ul li:nth-last-child(-n+4) { margin-bottom:0px;}
	.products-link .column-flex { justify-content:inherit;}
	.products-link ul li a h3 { padding:10px 5px 0;}
}
@media screen and (max-width:600px){
	.products-link ul li { width:33.33%;}
	.products-link ul li:nth-last-child(-n+4) { margin-bottom:30px;}
	.products-link ul li:nth-last-child(-n+3) { margin-bottom:0px;}
	.products-link ul li a h3 { padding:10px 5px 0;}
}

.characteristic { padding-bottom:40px;}
.characteristic img { max-width:100%; height:auto;}
.CharacteristicL { width:480px;}
.CharacteristicL-over { width:100%;}
.CharacteristicL dl, .CharacteristicL-over dl { clear:both;}
.CharacteristicL dl dt, .CharacteristicL-over dl dt { font-size:1.125rem; line-height:140%; font-weight:bold; margin:0 0 20px 0; padding-left:1.5em; text-indent:-1.5em;}
.CharacteristicL dl dd, .CharacteristicL-over dl dd { line-height:160%; margin:-15px 0 20px 0; padding:0 0 0 1.8rem; word-break:break-all;}
.CharacteristicL dl dt, .CharacteristicL-over dl dt { counter-increment: count;}
.CharacteristicL dl dt:before, .CharacteristicL-over dl dt:before { content:counter(count) "　";}
.CharacteristicR { width:300px;}
.CharacteristicR img { margin:0 0 18px 0; max-width:100%;}
.CharacteristicR img:last-child { margin:0;}
@media screen and (max-width:900px){

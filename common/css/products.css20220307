#page-tab { padding:10px 0 0 0; background:#0062B2; height:50px; box-sizing:border-box; margin:0 0 40px 0;}
#page-tab ul { width:780px; margin:0 auto; height:40px;}
#page-tab ul li { margin:0; padding:0; width:25%; box-sizing:border-box; border-right:1px solid #0062B2; line-height:100%;}
#page-tab ul li:last-child { border:none;}
#page-tab ul li span { display:block; width:100%; background:#4D91C9; height:40px; text-align:center; color:#fff; font-size:0.938rem; line-height:100%; box-sizing:border-box; padding:12px 0 0 0; text-decoration:none; cursor:pointer;}
#page-tab ul li span:hover { text-decoration:none; background:#FFF; color:#005FB2;}

#page-tab2 { padding:10px 0 0 0; background:#0062B2; height:50px; box-sizing:border-box; margin:0 0 40px 0;}
#page-tab2 ul { width:780px; margin:0 auto; height:40px;}
#page-tab2 ul li { margin:0; padding:0; /*width:16.667%;*/ box-sizing:border-box; border-right:1px solid #0062B2; line-height:100%;}
#page-tab2 ul li:last-child { border:none;}
#page-tab2 ul li span { display:block; width:100%; background:#4D91C9; height:40px; text-align:center; color:#fff; font-size:0.938rem; line-height:100%; box-sizing:border-box; padding:12px 0 0 0; text-decoration:none; cursor:pointer;}
#page-tab2 ul li span:hover { text-decoration:none; background:#FFF; color:#005FB2;}

#page-tab2 ul li:nth-child(1) { width:14%;}
#page-tab2 ul li:nth-child(2) { width:14%;}
#page-tab2 ul li:nth-child(3) { width:14%;}
#page-tab2 ul li:nth-child(4) { width:14%;}
#page-tab2 ul li:nth-child(5) { width:22%;}
#page-tab2 ul li:nth-child(6) { width:22%;}

.characteristic #page-tab2 ul li:nth-child(1) span { text-decoration:none; background:#FFF; color:#005FB2;}
.use #page-tab2 ul li:nth-child(2) span { text-decoration:none; background:#FFF; color:#005FB2;}
.installation #page-tab2 ul li:nth-child(3) span { text-decoration:none; background:#FFF; color:#005FB2;}
.faq #page-tab2 ul li:nth-child(4) span { text-decoration:none; background:#FFF; color:#005FB2;}
.lineup #page-tab2 ul li:nth-child(5) span { text-decoration:none; background:#FFF; color:#005FB2;}
.option #page-tab2 ul li:nth-child(6) span { text-decoration:none; background:#FFF; color:#005FB2;}

#page-tab3 { padding:10px 0 0 0; background:#0062B2; height:50px; box-sizing:border-box; margin:0 0 40px 0;}
#page-tab3 ul { width:780px; margin:0 auto; height:40px;}
#page-tab3 ul li { margin:0; padding:0; width:25%; box-sizing:border-box; border-right:1px solid #0062B2; line-height:100%;}
#page-tab3 ul li:last-child { border:none;}

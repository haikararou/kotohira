	.CharacteristicR { width:100%; text-align:center;}
	.CharacteristicR img { /*width:80%;*/ max-width:80%;}
}
@media screen and (max-width:600px){
	.CharacteristicR img { /*width:100%;*/ max-width:100%;}
	.characteristic .ph_l { float:none; margin:0 0 0.5rem 0!important; display:block;}
	.characteristic .ph_r { float:none; margin:0 0 0.5rem 0!important; display:block;}
}


